# prompt by git
git_branch_name(){
  git symbolic-ref HEAD 2>/dev/null | cut -d"/" -f 3
}
git_branch_prompt(){
  local branch=`git_branch_name`
  if [ $branch ]; then printf " [%s]" $branch; fi
}
