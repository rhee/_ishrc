# lsof, find processes using a specific tcp port
alias lsof-port-listeners='_lsof_port_listeners'
_lsof_port_listeners(){(
    port=$1
    shift
    # disable uid, dns, port name resolv
    lsof -l -n -P -i:$port
)}

